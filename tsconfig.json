{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react-native",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "moduleDetection": "auto",
    "baseUrl": ".",
    "paths": {
      "@/*":[
        "app/*"
      ],
      "@c/*": [
        "components/*"
      ],
      "@cc/*": [
        "components/controls/*"
      ],
      "@a/*": [
        "assets/*"
      ],
      "@u/*": [
        "utilities/*"
      ],
      "@s/*": [
        "splash/*"
      ],
    }
  }
}
