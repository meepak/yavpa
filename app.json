{
  "expo": {
    "name": "MyPath - Animate Every Stroke",
    "slug": "yavpa",
    "scheme": "mypath-aes",
    "version": "1.0.0",
    "orientation": "portrait",
    "androidStatusBar": {
      "translucent": true,
      "barStyle": "light-content",
      "backgroundColor": "#020935"
    },
    "icon": "./assets/icon.png",
    "backgroundColor": "#020935",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#020935"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "au.mahat.mypath"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#020935"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "au.mahat.mypath"
    },
    "extra": {
      "eas": {
        "projectId": "cc801c2c-70cf-4ae5-9b04-ad99cb74de4e"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "owner": "meepak",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/cc801c2c-70cf-4ae5-9b04-ad99cb74de4e"
    }
  }
}
