{
  "expo": {
    "name": "mypath",
    "slug": "yavpa",
    "scheme": "mypath",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8f0fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "androidStatusBar": {
      "hidden": true
    },
    "ios": {
      "requireFullScreen": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.meepak.mypath"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8f0fff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.meepak.mypath"
    },
    "extra": {
      "eas": {
        "projectId": "cc801c2c-70cf-4ae5-9b04-ad99cb74de4e"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "owner": "meepak",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/cc801c2c-70cf-4ae5-9b04-ad99cb74de4e"
    }
  }
}
